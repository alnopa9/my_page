<?xml version="1.0" encoding="UTF-8"?>
<schedule>
    <division type="metropolitan">
        <organization team="teamName">
            <season type="reg">
                <year term="2018">
                    <months class="month">
                        <game number="gameNumber" iosDate="yyyy/mm/dd">
                            <day></day>
                            <m></m>
                            <date></date>
                            <time></time>
                            <destination></destination>
                            <against></against>
                            <outcome>
                                <win pts="2">Winning Team</win>
                                <lose pts="0 or 1">Losing Team</lose>
                            </outcome>
                        </game>
                    </months> 
                </year>
            </season>
        </organization>
    </division>
</schedule>
